@model IEnumerable<Department>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title> Index - All Departments</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Name</th>

            </tr>

        </thead>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
            </tr>
        }
    </table>
</body>
</html>
